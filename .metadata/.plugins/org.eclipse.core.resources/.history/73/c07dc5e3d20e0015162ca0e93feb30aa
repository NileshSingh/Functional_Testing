	    driver = new FirefoxDriver();
	    baseUrl = "http://10.129.50.4/";
	    driver.manage().timeouts().implicitlyWait(30, TimeUnit.SECONDS);
	  }
	  @Test
	  public void testResetPassword() throws Exception {
	    driver.get(baseUrl + "/");

}
